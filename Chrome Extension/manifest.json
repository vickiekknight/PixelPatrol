{
  "manifest_version": 3,
  "name": "AI Image Detector",
  "version": "1.0",
  "description": "A Chrome extension that detects AI-generated images.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["popup.js"]
    }
  ]
}
