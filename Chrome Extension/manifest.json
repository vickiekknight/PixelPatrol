{
    "manifest_version": 2,
    "name": "AI Image Detector",
    "version": "1.0",
    "description": "Detect AI-generated or deepfake images",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],
    "background": {
      "scripts": ["background.js"]
    }
  }
  